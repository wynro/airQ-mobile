<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.app.team103.airQ">
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        
        <activity
            android:name="com.app.team103.airQ.MainActivity"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.app.team103.airQ.InputDataActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.app.team103.airQ.VisualizeDataActivity"
            android:screenOrientation="portrait" />
        <activity android:name="com.app.team103.airQ.AddSymptomActivity" android:screenOrientation="portrait" />
        <activity android:name="com.app.team103.airQ.AddEventActivity" android:screenOrientation="portrait" />
    </application>

</manifest>